{
  "version": 2,
  "builds": [
    {
      "src": "api/index_updated.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/miniapp.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index_updated.py"
    },
    {
      "src": "/miniapp/(.*)",
      "dest": "api/miniapp.py"
    },
    {
      "src": "/",
      "dest": "api/index_updated.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}