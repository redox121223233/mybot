{
  "version": 2,
  "builds": [
    {
      "src": "bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "bot.py"
    }
  ],
  "env": {
    "BOT_TOKEN": "@bot_token",
    "REQUIRED_CHANNEL": "@required_channel",
    "GITHUB_REPO": "@github_repo"
  }
}
